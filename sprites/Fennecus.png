<script>
    async function defineCurrencies() {
        let _response = await fetch("?a=General&define_currencies", {
            method: "POST",
            headers: {
                "Content-Type": "application/json",
            }
        });
        await _response.json();
    }

    document.addEventListener("DOMContentLoaded", async (event) => {
        await defineCurrencies();
        setInterval(async () => {
            await defineCurrencies();
        }, 15000);
    });
</script>
